{
  "name": "concurrent-json-fetcher",
  "description": "Concurrently fetch from an array of URLs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": "bin/index.js",
  "scripts": {
    "build": "npm run clean && babel -d ./dist ./src -s --ignore \"src/**/*.test.js\"",
    "clean": "rm -rf dist && mkdir dist",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint --fix \"src/**/*.js\"",
    "start": "npm run build && node ./src",
    "test": "mocha --require @babel/register --recursive \"./{,!(node_modules)/**}/*.test.js\""
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "chai": "^4.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0"
  },
  "author": "Daniel Ben Avraham",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leinaDBA/concurrent-json-fetcher.git"
  }
}
